[08:29, 09/02/2022] Prasanna Mam Lab: int main()
{
   int ar[100],i,n;
   printf("enter the size of array");
   scanf("%d",&n);
printf("enter the elements of the array\n");
for(i=0;i<n;i++)
{
scanf("%d",&ar[i]);
}
printf("even no is the array are");
for(i=0;i<n;i++)
{
    if(ar[i]%2==0)
    {
        printf("%d",ar[i]);
    }
}
   printf("odd no is the array are");
for(i=0;i<n;i++)
{
    if(ar[i]%2!=0)
    {
        printf("%d",ar[i]);
    }
}
    return 0;
}
